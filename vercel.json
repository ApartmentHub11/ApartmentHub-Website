{
    "redirects": [
        {
            "source": "/:path(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "apartmenthub.nl"
                }
            ],
            "destination": "https://www.apartmenthub.nl/:path",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/robots.txt",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain"
                }
            ]
        },
        {
            "source": "/sitemap.xml",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}